<Window x:Class="Choreo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:diag="clr-namespace:System.Diagnostics;assembly=WindowsBase"
        xmlns:local="clr-namespace:Choreo"
        mc:Ignorable="d"
        Height="450" Width="800" WindowStyle="None" Loaded="Window_Loaded">
    <!--        Height="450" Width="800" WindowStyle="None" WindowState="Maximized" ResizeMode="NoResize" Topmost="True" Style="{DynamicResource WinAll}"> -->
    <Window.DataContext>
        <Binding Source="{StaticResource ChoreoViewModel}"/>
    </Window.DataContext>
</Window>
